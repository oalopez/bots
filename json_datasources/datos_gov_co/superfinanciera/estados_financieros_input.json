{
    "version": "1.0",
    "description": "Crawling configuration for Financial Statements for SuperFinanciera taken datos.gov.co",
    "crawl": {
        "type": "http-all-records",
        "rules": {
            "url": "https://www.datos.gov.co/resource/mxk5-ce6w.json",
            "method": "GET",
            "delay": 0,
            "timeout": 30000,
            "params": {
                "$where": "fecha_corte between {#year#}-12-31T00:00:00 and {#year#}-12-31T23:59:59",
                "moneda": "0"
            }
        }
    }
}