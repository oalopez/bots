{
    "version": "1.0",
    "description": "CSV definition for Financial Statements.",
    "output-file": {
        "type": "csv",
        "output-folder": "./output",
        "output-file-pattern": "eeff-superfinanciera.csv",
        "separator": "~{<",
        "encoding": "utf-8",
        "header": true,
        "field-mapping": [
            "TIPO_IDENTIFICACION",
            "IDENTIFICACION",
            "RAZON",
            "CODIGO_CUENTA",
            "DESCRIPCION_CUENTA",
            "VALOR",
            "NIIF",
            "FECHA_FINANZAS"
        ]
    }
}